local CustomTab = Window:Tab({ Title = "Custom Item", Icon = "wand" })

local customItemId = ""

CustomTab:Input({
    Title = "ID do Acess√≥rio",
    Default = "",
    Placeholder = "Digite o ID do item...",
    Callback = function(v)
        customItemId = v:match("%d+") or v
    end
})

CustomTab:Button({
    Title = "‚ú® Equipar Item Personalizado",
    Callback = function()
        if customItemId == "" then
            WindUI:Notify({
                Title = "Erro",
                Content = "Digite um ID v√°lido primeiro!",
                Duration = 3,
                Image = "error"
            })
            return
        end
        
        local id = tonumber(customItemId)
        if not id or id <= 0 then
            WindUI:Notify({
                Title = "Erro",
                Content = "ID inv√°lido!",
                Duration = 3,
                Image = "error"
            })
            return
        end
        
        pcall(function()
            local success, accessory = pcall(function()
                return game:GetObjects("rbxassetid://" .. id)[1]
            end)
            
            if success and accessory then
                WeldAccessory(LocalPlayer.Character, accessory)
                WindUI:Notify({
                    Title = "Sucesso!",
                    Content = "Item " .. id .. " equipado!",
                    Duration = 3,
                    Image = "success"
                })
            else
                WindUI:Notify({
                    Title = "Erro",
                    Content = "N√£o foi poss√≠vel carregar o ID",
                    Duration = 3,
                    Image = "error"
                })
            end
        end)
    end
})

CustomTab:Button({
    Title = "üóëÔ∏è Limpar ID",
    Callback = function()
        customItemId = ""
        WindUI:Notify({
            Title = "Custom Item",
            Content = "ID limpo!",
            Duration = 2,
            Image = "success"
        })
    end
})

CustomTab:Divider({ Title = "IDs Populares" })

