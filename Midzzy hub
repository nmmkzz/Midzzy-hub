-- ============= TAB EMOTES =============
local EmoteTab = Window:Tab({ Title = "Emotes", Icon = "music" })

EmoteTab:Divider({ Title = "üéÆ Emotes Premium - " .. #Emotes .. " dispon√≠veis" })

-- Barra de pesquisa
local searchEmoteTerm = ""
EmoteTab:Input({
    Title = "üîç Pesquisar Emote",
    Default = "",
    Placeholder = "Nome do emote ou ID...",
    Callback = function(v)
        searchEmoteTerm = v:lower()
        -- Recriar bot√µes com filtro
        recreateEmoteButtons()
    end
})

-- Bot√µes de filtro
EmoteTab:Divider({ Title = "Filtros" })

local filterMode = "all" -- all, favorites
EmoteTab:Dropdown({
    Title = "üìÅ Mostrar",
    Values = {"Todos os Emotes", "Apenas Favoritos"},
    Default = 1,
    Callback = function(v)
        filterMode = v == "Todos os Emotes" and "all" or "favorites"
        recreateEmoteButtons()
    end
})

EmoteTab:Divider({ Title = "‚≠ê Emotes Favoritos" })

-- Fun√ß√£o para favoritar/desfavoritar
local function toggleFavorite(emoteId)
    local index = table.find(FavoritedEmotes, emoteId)
    if index then
        table.remove(FavoritedEmotes, index)
        WindUI:Notify({
            Title = "Favoritos",
            Content = "Emote removido dos favoritos!",
            Duration = 2,
            Image = "info"
        })
    else
        table.insert(FavoritedEmotes, emoteId)
        WindUI:Notify({
            Title = "Favoritos",
            Content = "Emote adicionado aos favoritos!",
            Duration = 2,
            Image = "success"
        })
    end
    writefile(favoriteFileName, HttpService:JSONEncode(FavoritedEmotes))
    recreateEmoteButtons()
end

-- Container para bot√µes de emote
local emoteContainer = nil
local emoteScrollFrame = nil

local function recreateEmoteButtons()
    if emoteContainer then
        emoteContainer:Destroy()
        emoteScrollFrame:Destroy()
    end
    
    -- Criar ScrollFrame
    emoteScrollFrame = Instance.new("ScrollingFrame")
    emoteScrollFrame.Size = UDim2.new(1, 0, 1, -30)
    emoteScrollFrame.Position = UDim2.new(0, 0, 0, 0)
    emoteScrollFrame.BackgroundTransparency = 1
    emoteScrollFrame.BorderSizePixel = 0
    emoteScrollFrame.ScrollBarThickness = 4
    emoteScrollFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
    emoteScrollFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
    
    -- Container com GridLayout
    emoteContainer = Instance.new("Frame")
    emoteContainer.Size = UDim2.new(1, 0, 0, 0)
    emoteContainer.BackgroundTransparency = 1
    emoteContainer.AutomaticSize = Enum.AutomaticSize.Y
    emoteContainer.Parent = emoteScrollFrame
    
    local grid = Instance.new("UIGridLayout")
    grid.CellSize = UDim2.new(0, 110, 0, 120)
    grid.CellPadding = UDim2.new(0, 8, 0, 8)
    grid.FillDirection = Enum.FillDirection.Horizontal
    grid.HorizontalAlignment = Enum.HorizontalAlignment.Left
    grid.VerticalAlignment = Enum.VerticalAlignment.Top
    grid.SortOrder = Enum.SortOrder.LayoutOrder
    grid.Parent = emoteContainer
    
    -- Filtrar emotes
    local filteredEmotes = {}
    for _, emote in ipairs(Emotes) do
        local shouldShow = true
        
        -- Filtro de pesquisa
        if searchEmoteTerm ~= "" then
            if not emote.name:lower():find(searchEmoteTerm) and not tostring(emote.id):find(searchEmoteTerm) then
                shouldShow = false
            end
        end
        
        -- Filtro de favoritos
        if shouldShow and filterMode == "favorites" then
            if not table.find(FavoritedEmotes, emote.id) then
                shouldShow = false
            end
        end
        
        if shouldShow then
            table.insert(filteredEmotes, emote)
        end
    end
    
    -- Criar bot√µes para cada emote
    for i, emote in ipairs(filteredEmotes) do
        local isFavorite = table.find(FavoritedEmotes, emote.id) ~= nil
        
        local emoteFrame = Instance.new("Frame")
        emoteFrame.Size = UDim2.new(0, 110, 0, 120)
        emoteFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 35)
        emoteFrame.BackgroundTransparency = 0.2
        emoteFrame.BorderSizePixel = 0
        emoteFrame.Parent = emoteContainer
        
        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(0, 8)
        corner.Parent = emoteFrame
        
        -- Imagem do emote
        local emoteImage = Instance.new("ImageLabel")
        emoteImage.Size = UDim2.new(1, -20, 0, 70)
        emoteImage.Position = UDim2.new(0, 10, 0, 5)
        emoteImage.BackgroundTransparency = 1
        emoteImage.Image = "rbxthumb://type=Asset&id=" .. emote.id .. "&w=150&h=150"
        emoteImage.ScaleType = Enum.ScaleType.Fit
        emoteImage.Parent = emoteFrame
        
        -- Nome do emote
        local nameLabel = Instance.new("TextLabel")
        nameLabel.Size = UDim2.new(1, -10, 0, 30)
        nameLabel.Position = UDim2.new(0, 5, 0, 80)
        nameLabel.BackgroundTransparency = 1
        nameLabel.Text = emote.name
        nameLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        nameLabel.TextSize = 11
        nameLabel.Font = Enum.Font.GothamBold
        nameLabel.TextWrapped = true
        nameLabel.TextScaled = true
        nameLabel.Parent = emoteFrame
        
        -- Pre√ßo
        local priceLabel = Instance.new("TextLabel")
        priceLabel.Size = UDim2.new(0.5, -2, 0, 15)
        priceLabel.Position = UDim2.new(0, 5, 0, 100)
        priceLabel.BackgroundTransparency = 1
        priceLabel.Text = emote.price > 0 and "üí∞ " .. emote.price or "üéÅ Gr√°tis"
        priceLabel.TextColor3 = emote.price > 0 and Color3.fromRGB(255, 200, 100) or Color3.fromRGB(100, 255, 100)
        priceLabel.TextSize = 10
        priceLabel.Font = Enum.Font.GothamBold
        priceLabel.TextXAlignment = Enum.TextXAlignment.Left
        priceLabel.Parent = emoteFrame
        
        -- Bot√£o favoritar
        local favButton = Instance.new("TextButton")
        favButton.Size = UDim2.new(0, 20, 0, 20)
        favButton.Position = UDim2.new(1, -25, 0, 5)
        favButton.BackgroundTransparency = 1
        favButton.Text = isFavorite and "‚òÖ" or "‚òÜ"
        favButton.TextColor3 = isFavorite and Color3.fromRGB(255, 200, 0) or Color3.fromRGB(200, 200, 200)
        favButton.TextSize = 18
        favButton.Font = Enum.Font.GothamBold
        favButton.Parent = emoteFrame
        
        favButton.MouseButton1Click:Connect(function()
            toggleFavorite(emote.id)
        end)
        
        -- Bot√£o tocar emote
        local playButton = Instance.new("TextButton")
        playButton.Size = UDim2.new(1, 0, 1, 0)
        playButton.BackgroundTransparency = 1
        playButton.Text = ""
        playButton.Parent = emoteFrame
        
        playButton.MouseButton1Click:Connect(function()
            PlayEmote(emote.name, emote.id)
            WindUI:Notify({
                Title = "Emote",
                Content = "Tocando: " .. emote.name,
                Duration = 2,
                Image = "success"
            })
        end)
    end
    
    -- Adicionar ao tab
    emoteScrollFrame.Parent = EmoteTab.Container or EmoteTab.Frame
end

-- Inicializar bot√µes de emote
task.spawn(function()
    recreateEmoteButtons()
end)

