-- Ordenar emotes por pre√ßo
table.sort(Emotes, function(a, b)
    return a.price < b.price
end)

-- ============= FUN√á√ïES DE EMOTE =============
local FavoritedEmotes = {}
local favoriteFileName = "FavoritedEmotes.txt"

-- Carregar favoritos
if isfile(favoriteFileName) then
    if not pcall(function()
        FavoritedEmotes = HttpService:JSONDecode(readfile(favoriteFileName))
    end) then
        FavoritedEmotes = {}
    end
else
    writefile(favoriteFileName, HttpService:JSONEncode(FavoritedEmotes))
end

-- Atualizar formato antigo de favoritos
local UpdatedFavorites = {}
for i, name in pairs(FavoritedEmotes) do
    if typeof(name) == "string" then
        for i, emote in pairs(Emotes) do
            if emote.name == name then
                table.insert(UpdatedFavorites, emote.id)
                break
            end
        end
    end
end
if #UpdatedFavorites ~= 0 then
    FavoritedEmotes = UpdatedFavorites
    writefile(favoriteFileName, HttpService:JSONEncode(FavoritedEmotes))
end

-- Fun√ß√£o para tocar emote
local function PlayEmote(name, id)
    local character = LocalPlayer.Character
    if not character then return end
    
    local humanoid = character:FindFirstChildOfClass("Humanoid")
    if not humanoid then return end
    
    if character.Humanoid.RigType ~= Enum.HumanoidRigType.R6 then
        local success, err = pcall(function()
            humanoid:PlayEmoteAndGetAnimTrackById(id)
        end)
        if not success then
            local desc = humanoid:FindFirstChildOfClass("HumanoidDescription")
            if desc then
                desc:AddEmote(name, id)
                humanoid:PlayEmoteAndGetAnimTrackById(id)
            end
        end
    else
        WindUI:Notify({
            Title = "Erro",
            Content = "Voc√™ precisa estar em R15 para usar emotes!",
            Duration = 3,
            Image = "error"
        })
    end
end

