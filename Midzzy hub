-- WindUI Carregar
local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

-- Vari√°veis globais
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local VirtualUser = game:GetService("VirtualUser")
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local CoreGui = game:GetService("CoreGui")
local TweenService = game:GetService("TweenService")
local HttpService = game:GetService("HttpService")
local camera = workspace.CurrentCamera
local MarketplaceService = game:GetService("MarketplaceService")
local UserInputService = game:GetService("UserInputService")
local ContextActionService = game:GetService("ContextActionService")
local GuiService = game:GetService("GuiService")
local StarterGui = game:GetService("StarterGui")

-- Destruir GUIs antigas
for _, gui in pairs(CoreGui:GetChildren()) do
    if gui:IsA("ScreenGui") and (gui.Name == "VisualWorks" or gui.Name == "WindUI" or gui:FindFirstChild("MainFrame") or gui.Name == "Emotes") then
        gui:Destroy()
    end
end

-- Anti AFK e FPS Cap
setfpscap(444)
LocalPlayer.Idled:Connect(function()
    VirtualUser:CaptureController()
    VirtualUser:ClickButton2(Vector2.new())
end)

-- ============= FUN√á√ïES DE ANIMA√á√ÉO =============

-- Verificar R15
if not game.Players.LocalPlayer.Character or game.Players.LocalPlayer.Character:WaitForChild("Humanoid").RigType ~= Enum.HumanoidRigType.R15 then 
    game.StarterGui:SetCore("SendNotification", {Title = "R6", Text = "You're on R6, bro. Change to R15!", Duration = 60})
end

-- Sistema de Notifica√ß√£o
local Notifbro = {}
function Notify(titletxt, text, time)
    coroutine.wrap(function()
        local GUI = Instance.new("ScreenGui")
        local Main = Instance.new("Frame", GUI)
        local title = Instance.new("TextLabel", Main)
        local message = Instance.new("TextLabel", Main)

        GUI.Name = "BackgroundNotif"
        GUI.Parent = CoreGui

        local sw = workspace.CurrentCamera.ViewportSize.X
        local sh = workspace.CurrentCamera.ViewportSize.Y
        local nh = sh / 7
        local nw = sw / 5

        Main.Name = "MainFrame"
        Main.BackgroundColor3 = Color3.new(0.156863, 0.156863, 0.156863)
        Main.BackgroundTransparency = 0.2
        Main.BorderSizePixel = 0
        Main.Size = UDim2.new(0, nw, 0, nh)

        title.BackgroundColor3 = Color3.new(0, 0, 0)
        title.BackgroundTransparency = 0.9
        title.Size = UDim2.new(1, 0, 0, nh / 2)
        title.Font = Enum.Font.GothamBold
        title.Text = titletxt
        title.TextColor3 = Color3.new(1, 1, 1)
        title.TextScaled = true

        message.BackgroundColor3 = Color3.new(0, 0, 0)
        message.BackgroundTransparency = 1
        message.Position = UDim2.new(0, 0, 0, nh / 2)
        message.Size = UDim2.new(1, 0, 1, -nh / 2)
        message.Font = Enum.Font.Gotham
        message.Text = text
        message.TextColor3 = Color3.new(1, 1, 1)
        message.TextScaled = true

        local offset = 50
        for _, notif in ipairs(Notifbro) do
            offset = offset + notif.Size.Y.Offset + 10
        end

        Main.Position = UDim2.new(1, 5, 0, offset)
        table.insert(Notifbro, Main)

        task.wait(0.1)
        Main:TweenPosition(UDim2.new(1, -nw, 0, offset), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, 0.5, true)
        Main:TweenSize(UDim2.new(0, nw * 1.06, 0, nh * 1.06), Enum.EasingDirection.Out, Enum.EasingStyle.Elastic, 0.5, true)
        task.wait(0.1)
        Main:TweenSize(UDim2.new(0, nw, 0, nh), Enum.EasingDirection.Out, Enum.EasingStyle.Elastic, 0.2, true)

        task.wait(time)

        Main:TweenSize(UDim2.new(0, nw * 1.06, 0, nh * 1.06), Enum.EasingDirection.In, Enum.EasingStyle.Elastic, 0.2, true)
        task.wait(0.2)
        Main:TweenSize(UDim2.new(0, nw, 0, nh), Enum.EasingDirection.In, Enum.EasingStyle.Elastic, 0.2, true)
        task.wait(0.2)
        Main:TweenPosition(UDim2.new(1, 5, 0, offset), Enum.EasingDirection.In, Enum.EasingStyle.Bounce, 0.5, true)
        task.wait(0.1)

        GUI:Destroy()
        for i, notif in ipairs(Notifbro) do
            if notif == Main then
                table.remove(Notifbro, i)
                break
            end
        end

        for i, notif in ipairs(Notifbro) do
            local newOffset = 50 + (nh + 10) * (i - 1)
            notif:TweenPosition(UDim2.new(1, -nw, 0, newOffset), Enum.EasingDirection.Out, Enum.EasingStyle.Bounce, 0.5, true)
        end
    end)()
end

-- ============= FUN√á√ïES DE REMO√á√ÉO DE ACESS√ìRIOS =============
local function RemoveAllAccessories()
    local character = LocalPlayer.Character
    if not character then return end
    
    for _, accessory in pairs(character:GetChildren()) do
        if accessory:IsA("Accessory") or accessory:IsA("Hat") or accessory:IsA("Tool") then
            accessory:Destroy()
        end
    end
    
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("Weld") and part.Name == "HeadWeld" then
            part:Destroy()
        end
    end
    
    WindUI:Notify({
        Title = "Remover Acess√≥rios",
        Content = "Todos os acess√≥rios foram removidos!",
        Duration = 3,
        Image = "success"
    })
end

-- ============= FUN√á√ïES DE WELD =============
local function WeldAccessory(character, accessory)
    for _, child in pairs(character:GetChildren()) do
        if child:IsA("Accessory") and child.Name == accessory.Name then
            child:Destroy()
        end
    end
    
    accessory.Parent = character
    local handle = accessory:FindFirstChild("Handle")
    
    if handle then
        local attachment = handle:FindFirstChildOfClass("Attachment")
        if attachment then
            local head = character:FindFirstChild("Head")
            if head then
                for _, headAttachment in pairs(head:GetChildren()) do
                    if headAttachment:IsA("Attachment") and headAttachment.Name == attachment.Name then
                        local weld = Instance.new("Weld")
                        weld.Name = "HeadWeld_" .. accessory.Name
                        weld.Part0 = head
                        weld.Part1 = handle
                        weld.C0 = headAttachment.CFrame
                        weld.C1 = attachment.CFrame
                        weld.Parent = head
                        break
                    end
                end
            end
        else
            local head = character:FindFirstChild("Head")
            if head then
                local weld = Instance.new("Weld")
                weld.Name = "HeadWeld_" .. accessory.Name
                weld.Part0 = head
                weld.Part1 = handle
                weld.C0 = CFrame.new(0, 0.5, 0)
                weld.C1 = accessory.AttachmentPoint or CFrame.new(0, 0.5, 0)
                weld.Parent = head
            end
        end
    end
end

-- ============= FUN√á√ïES DE FACE =============
local function ChangeFace(faceId)
    local character = LocalPlayer.Character
    if not character then return end
    
    local head = character:FindFirstChild("Head")
    if not head then return end
    
    local face = head:FindFirstChild("face") or head:FindFirstChild("Face")
    if not face then
        face = Instance.new("Decal")
        face.Name = "face"
        face.Face = Enum.NormalId.Front
        face.Parent = head
    end
    
    face.Texture = "rbxassetid://" .. tostring(faceId)
end

-- ============= FUN√á√ïES DE KORBLOX =============
local function ApplyKorblox(side)
    local character = LocalPlayer.Character
    if not character then return end
    
    local function ensurePart(partName)
        local part = character:FindFirstChild(partName)
        if not part then
            part = Instance.new("Part")
            part.Name = partName
            part.Parent = character
        end
        return part
    end
    
    if side == "right" then
        local rightLowerLeg = ensurePart("RightLowerLeg")
        rightLowerLeg.MeshId = "rbxassetid://902942093"
        rightLowerLeg.Transparency = 1
        rightLowerLeg.Size = Vector3.new(1, 1, 1)
        
        local rightUpperLeg = ensurePart("RightUpperLeg")
        rightUpperLeg.MeshId = "rbxassetid://902942096"
        rightUpperLeg.TextureID = "rbxassetid://902843398"
        rightUpperLeg.Transparency = 0
        
        local rightFoot = ensurePart("RightFoot")
        rightFoot.MeshId = "rbxassetid://902942089"
        rightFoot.Transparency = 1
        rightFoot.Size = Vector3.new(1, 1, 1)
        
    elseif side == "left" then
        local leftLowerLeg = ensurePart("LeftLowerLeg")
        leftLowerLeg.MeshId = "rbxassetid://101851582"
        leftLowerLeg.TextureID = "rbxassetid://101851582"
        leftLowerLeg.Transparency = 0
        leftLowerLeg.Size = Vector3.new(1, 1, 1)
        
        local leftUpperLeg = ensurePart("LeftUpperLeg")
        leftUpperLeg.MeshId = "rbxassetid://101851582"
        leftUpperLeg.TextureID = "rbxassetid://101851582"
        leftUpperLeg.Transparency = 0
        
        local leftFoot = ensurePart("LeftFoot")
        leftFoot.MeshId = "rbxassetid://101851582"
        leftFoot.TextureID = "rbxassetid://101851582"
        leftFoot.Transparency = 0
        leftFoot.Size = Vector3.new(1, 1, 1)
        
        local leftHip = character:FindFirstChild("Left Hip") or character:FindFirstChild("LeftHip")
        if leftHip and leftHip:IsA("Motor6D") then
            leftHip.C0 = CFrame.new(-1, 1, 0) * CFrame.Angles(0, math.rad(-90), math.rad(-10))
        end
    end
end

local function RemoveKorblox()
    local character = LocalPlayer.Character
    if not character then return end
    
    local rightLowerLeg = character:FindFirstChild("RightLowerLeg")
    if rightLowerLeg then
        rightLowerLeg.MeshId = ""
        rightLowerLeg.TextureID = ""
        rightLowerLeg.Transparency = 0
        rightLowerLeg.Size = Vector3.new(1, 1, 1)
    end
    
    local rightUpperLeg = character:FindFirstChild("RightUpperLeg")
    if rightUpperLeg then
        rightUpperLeg.MeshId = ""
        rightUpperLeg.TextureID = ""
        rightUpperLeg.Transparency = 0
    end
    
    local rightFoot = character:FindFirstChild("RightFoot")
    if rightFoot then
        rightFoot.MeshId = ""
        rightFoot.TextureID = ""
        rightFoot.Transparency = 0
        rightFoot.Size = Vector3.new(1, 1, 1)
    end
    
    local leftLowerLeg = character:FindFirstChild("LeftLowerLeg")
    if leftLowerLeg then
        leftLowerLeg.MeshId = ""
        leftLowerLeg.TextureID = ""
        leftLowerLeg.Transparency = 0
        leftLowerLeg.Size = Vector3.new(1, 1, 1)
    end
    
    local leftUpperLeg = character:FindFirstChild("LeftUpperLeg")
    if leftUpperLeg then
        leftUpperLeg.MeshId = ""
        leftUpperLeg.TextureID = ""
        leftUpperLeg.Transparency = 0
    end
    
    local leftFoot = character:FindFirstChild("LeftFoot")
    if leftFoot then
        leftFoot.MeshId = ""
        leftFoot.TextureID = ""
        leftFoot.Transparency = 0
        leftFoot.Size = Vector3.new(1, 1, 1)
    end
    
    local leftHip = character:FindFirstChild("Left Hip") or character:FindFirstChild("LeftHip")
    if leftHip and leftHip:IsA("Motor6D") then
        leftHip.C0 = CFrame.new(-1, 1, 0) * CFrame.Angles(0, math.rad(-90), 0)
    end
    
    WindUI:Notify({
        Title = "Korblox",
        Content = "Korblox removido!",
        Duration = 3,
        Image = "success"
    })
end

